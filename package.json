    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/platform-server": "5.2.10",
    "@angular/router": "5.2.10",
    "@angular/tsc-wrapped": "^4.4.6",
    "@angular/upgrade": "5.2.10",
    "bootstrap": "4.1.0",
    "jquery": "3.3.1",
    "bootbox": "4.4.0",
    "core-js": "^2.4.1",
    "rxjs": "5.5.10",
    "systemjs": "0.21.3",
    "zone.js": "^0.8.26",
